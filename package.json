    "remark-preset-lint-markdown-style-guide": "5.1.3",
    "remark-preset-lint-recommended": "6.1.3",
    "remark-rehype": "11.0.0",
    "rimraf": "5.0.1",
    "shelljs": "0.8.5",
    "sonarqube-scanner": "3.1.0",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typedoc": "0.25.1",
    "typescript": "5.1.6",
    "unified": "11.0.3",
    "weak-napi": "2.0.2"
  }
}
