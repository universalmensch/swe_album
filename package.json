{
  "name": "buch",
  "version": "2023.10.0",
  "description": "Beispiel",
  "author": "Juergen Zimmermann",
  "private": true,
  "license": "GPL-3.0",
  "exports": "./dist/src/main.js",
  "type": "module",
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=10.1.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "nest build",
    "start": "node dist/src/main.js",
    "pre:dev": "npm run copy-resources",
    "dev": "nest start --watch",
    "swc:install": "npm i -D @swc/cli @swc/core @swc/jest",
    "swc": "nest start --builder swc --watch",
    "swc:check": "nest start --builder swc --type-check --watch",
    "debug": "nest start --debug --watch",
    "test:ORIG": "jest",
    "pretest": "npm run copy-resources",
    "test": "set LOG_DEFAULT=true&& set START_DB_SERVER=false&& jest --runInBand",
    "test:coverage": "npm t -- --coverage",
    "test:istanbul": "npm run test:coverage",
    "test:dbserver": "set LOG_DEFAULT=true&& jest --runInBand",
    "test:detectOpenHandles": "set LOG_DEFAULT=true&& jest --runInBand --detectOpenHandles",
    "test:forceExit": "set LOG_DEFAULT=true&& jest --runInBand --forceExit",
    "test:logdebug": "jest --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "asciidoctor": "node scripts/asciidoctor",
    "clinic": "clinic doctor -- node dist/src/main.js",
    "clean": "rimraf dist log/server.log",
    "copy-resources": "node scripts/copy-resources",
    "depcheck": "depcheck",
    "eslint": "eslint src __tests__",
    "eslint:debug": "eslint --debug src __tests__",
    "invoke-webrequest": "pwsh -Command Invoke-WebRequest https://localhost:3000/00000000-0000-0000-0000-000000000001 -SslProtocol Tls13 -SkipCertificateCheck",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "prepack": "npm run clean && npm run copy-resources && npm run tsc",
    "pack": "pack build juergenzimmermann/buch:1.0.0 --builder paketobuildpacks/builder:full",
    "postpack": "docker tag juergenzimmermann/buch:1.0.0 juergenzimmermann/buch:latest",
    "pack:clear-cache": "pack build juergenzimmermann/buch:1.0.0 --builder paketobuildpacks/builder:base --clear-cache",
    "pack:inspect": "pack inspect juergenzimmermann/buch:1.0.0",
    "prettier": "prettier --cache --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "prettier:check": "prettier --cache --check \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "readme2html": "node scripts/readme2html",
    "revealjs": "asciidoctor-revealjs .extras/doc/folien/folien.adoc",
    "rmi": "docker rmi juergenzimmermann/buch:1.0.0",
    "snyk:auth": "echo https://app.snyk.io/account && snyk auth",
    "snyk": "snyk test",
    "sonar-scanner": "node scripts/sonar-scanner",
    "typedoc": "typedoc",
    "tsc": "tsc --pretty",
    "tsc:watch": "npm run tsc -- --watch",
    "type-coverage": "type-coverage --ignore-files __tests__/**/*.ts --ignore-files dist/**/*.ts --detail --show-relative-path"
  },
  "keywords": [
    "Node",
    "npm",
    "Nest",
    "REST",
    "GraphQL",
    "Apollo",
    "Axios",
    "TypeScript",
    "ESLint",
    "PostgreSQL",
    "TypeORM",
    "JWT",
    "Passport",
    "argon2",
    "Ajv",
    "CSP",
    "Clickjacking",
    "CORS",
    "pino",
    "nodemailer",
    "Jest",
    "ts-jest"
  ],
  "dependencies": {
    "@apollo/server": "4.9.3",
    "@js-temporal/polyfill": "0.4.4",
    "@nestjs/apollo": "12.0.9",
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.2.6",
    "@nestjs/core": "10.2.6",
    "@nestjs/graphql": "12.0.9",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/passport": "10.0.2",
    "@nestjs/platform-express": "10.2.6",
    "@nestjs/swagger": "7.1.12",
    "@nestjs/terminus": "10.1.1",
    "@nestjs/typeorm": "10.0.0",
    "argon2": "0.31.1",
    "axios": "1.5.0",
    "chalk": "5.3.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "figlet": "1.6.0",
    "form-data": "4.0.0",
    "fs-extra": "11.1.1",
    "graphql": "16.8.1",
    "helmet": "7.0.0",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "9.0.2",
    "mysql2": "3.6.1",
    "nodemailer": "6.9.5",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.11.3",
    "pino": "8.15.1",
    "pino-pretty": "10.2.0",
    "re2": "1.20.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "semver": "7.5.4",
    "sonic-boom": "3.3.0",
    "sqlite3": "5.1.6",
    "typeorm": "0.3.17",
    "typeorm-naming-strategies": "4.1.0",
    "uuid": "9.0.1",
    "xss": "1.0.14"
  },
  "devDependencies": {
    "@asciidoctor/reveal.js": "5.0.1",
    "@eslint/js": "8.50.0",
    "@graphql-eslint/eslint-plugin": "3.20.1",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@nestjs/cli": "10.1.18",
    "@nestjs/testing": "10.2.6",
    "@tsconfig/node18": "18.2.2",
    "@tsconfig/node20": "20.1.2",
    "@types/compression": "1.7.3",
    "@types/express": "4.17.18",
    "@types/figlet": "1.5.6",
    "@types/fs-extra": "11.0.2",
    "@types/jest": "29.5.5",
    "@types/js-yaml": "4.0.6",
    "@types/node": "20.6.4",
    "@types/nodemailer": "6.4.11",
    "@types/nodemailer-direct-transport": "1.0.33",
    "@types/nodemailer-smtp-transport": "2.7.6",
    "@types/passport-jwt": "3.0.10",
    "@types/passport-local": "1.0.36",
    "@types/shelljs": "0.8.12",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.4",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "asciidoctor": "3.0.0",
    "asciidoctor-kroki": "0.17.0",
    "depcheck": "1.4.7",
    "docker-compose": "0.24.2",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-n": "16.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "1.15.0",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-security-node": "1.1.1",
    "eslint-plugin-sonarjs": "0.21.0",
    "eslint-plugin-unicorn": "48.0.1",
